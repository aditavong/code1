int circleX = 50;


//happens once 
void setup() {
  //size of window (screen) - goes at the beginning
  size(640, 360);
  //variable goes here bc its a one time thing
  
}
//happens forever
void draw() {
  //background
  background(50);

  fill(255);
  ellipse(circleX, 180, 24, 24);
}





